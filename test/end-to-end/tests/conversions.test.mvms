/*---
description: >
  Tests primitive type conversions
runExportedFunction: 0
# assertionCount: 25
---*/
vmExport(0, run);

function run() {
  testConvertToNumber();
}

function testConvertToNumber() {
  assertEqual(+(1 + 1), 2);
  assertEqual(+(1.1 + 2), 3.1);
  assert(isNaN(+undefined));
  assert(isNaN(+{}));
  assertEqual(+null, 0);
